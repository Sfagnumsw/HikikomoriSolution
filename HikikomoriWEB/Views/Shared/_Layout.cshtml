<!DOCTYPE HTML>
<html>
<head>
    @await Html.PartialAsync("CssPartial")
    @await Html.PartialAsync("MetatagsPartial")
    <link rel="shortcut icon" href="~/reporting/favicon.ico">
</head>
<body>
    <div class="wrapper">
        @await Html.PartialAsync("HeaderPartial")
        <div id="main">
            <div class="container">
                <div class="row main-row">
                    <div class="col-8 col-12-medium imp-medium">
                        @RenderBody()
                    </div>
                    @await Html.PartialAsync("SidebarPartial") 
                </div>
            </div>
        </div>
        @await Html.PartialAsync("FooterPartial")
    </div>
    @await Html.PartialAsync("ScriptPartial")
</body>
</html>